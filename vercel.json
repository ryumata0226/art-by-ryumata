{
  "buildCommand": "npm run build",
  "outputDirectory": "public",
  "framework": "static",
  "routes": [{ "src": "/(.*)", "dest": "/$1" }],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "SAMEORIGIN" },
        { "key": "X-Content-Type-Options", "value": "nosniff" }
      ]
    }
  ]
}
